{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended","plugin:react/recommended"],
  "globals":{
    "React":true,
    "ReactDOM":true,
    "classNames":true,
    "$update":true,
    "jsPlumb":true
  },
  "env":{
    "browser":true,
    "node":true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules":{
    "no-console":1,
    "no-mixed-spaces-and-tabs":0,
    "react/display-name":0,
    "no-unused-vars":1,
    "no-debugger":1,
    "no-extra-boolean-cast":1,
    "no-empty":1,
    "no-inner-declarations":0
  }
}